type User {
    id: ID!
    username: String!
    email: String!
}

type Query {
    getAllUsers: [User]!
    getUser(id: ID!): User
}

type Mutation {
    createUser(username: String!, email: String!, password: String!): User!
    updateUser(id: ID!, username: String, email: String): User!
    deleteUser(id: ID!): Boolean!
}
